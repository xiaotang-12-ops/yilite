### 2ï¸âƒ£ å‰ç«¯æ¸²æŸ“é€»è¾‘ï¼ˆä¼˜å…ˆçº§ï¼‰

```javascript
// ä¼˜å…ˆçº§ï¼šcustom_colors > 3d_highlight > assembledMeshes > é»˜è®¤ç°è‰²
const highlightStepParts = () => {
  const currentNodes = currentStepData.value['3d_highlight'] || []
  const assembledNodes = assembledMeshes.value
  const customColors = currentStepData.value['custom_colors'] || {}  // â­ æ–°å¢
  
  model.traverse((child: any) => {
    if (child.isMesh) {
      const nodeName = child.name
      
      // â­ ä¼˜å…ˆçº§1ï¼šç”¨æˆ·è‡ªå®šä¹‰é¢œè‰²ï¼ˆæœ€é«˜ä¼˜å…ˆçº§ï¼‰
      if (customColors[nodeName]) {
        const color = customColors[nodeName]
        if (color === 'yellow') {
          child.material = new THREE.MeshStandardMaterial({
            color: 0xffff00,
            emissive: 0xffaa00,
            emissiveIntensity: 0.8
          })
        } else if (color === 'green') {
          child.material = new THREE.MeshStandardMaterial({
            color: 0x4CAF50,
            emissive: 0x2E7D32,
            emissiveIntensity: 0.3
          })
        } else if (color === 'gray') {
          child.material = new THREE.MeshStandardMaterial({
            color: 0xCCCCCC,
            transparent: true,
            opacity: 0.3
          })
        }
      }
      // â­ ä¼˜å…ˆçº§2ï¼š3d_highlightï¼ˆé»„è‰²ï¼‰
      else if (currentNodes.includes(nodeName)) {
        child.material = new THREE.MeshStandardMaterial({
          color: 0xffff00,
          emissive: 0xffaa00,
          emissiveIntensity: 0.8
        })
      }
      // â­ ä¼˜å…ˆçº§3ï¼šassembledMeshesï¼ˆç»¿è‰²ï¼‰
      else if (assembledNodes.includes(nodeName)) {
        child.material = new THREE.MeshStandardMaterial({
          color: 0x4CAF50,
          emissive: 0x2E7D32,
          emissiveIntensity: 0.3
        })
      }
      // â­ ä¼˜å…ˆçº§4ï¼šé»˜è®¤ç°è‰²
      else {
        child.material = new THREE.MeshStandardMaterial({
          color: 0xCCCCCC,
          transparent: true,
          opacity: 0.3
        })
      }
    }
  })
}
```

