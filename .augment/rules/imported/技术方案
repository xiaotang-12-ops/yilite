### 4ï¸âƒ£ é›¶ä»¶åˆ—è¡¨æå–é€»è¾‘

```javascript
// æå–æ‰€æœ‰é›¶ä»¶åˆ—è¡¨ï¼ˆä»3Dæ¨¡å‹ä¸­ï¼‰
const allPartsList = computed(() => {
  const parts: Array<{ nodeName: string; bomName: string }> = []
  
  if (!model) return parts
  
  // éå†3Dæ¨¡å‹çš„æ‰€æœ‰mesh
  model.traverse((child: any) => {
    if (child.isMesh) {
      const nodeName = child.name
      
      // å°è¯•ä»æ‰€æœ‰æ­¥éª¤çš„parts_usedä¸­æŸ¥æ‰¾é›¶ä»¶åç§°
      let bomName = nodeName  // é»˜è®¤ä½¿ç”¨node_name
      
      allSteps.value.forEach((step: any) => {
        // ç»„ä»¶è£…é…ï¼šä»parts_usedä¸­æŸ¥æ‰¾
        if (step.parts_used) {
          step.parts_used.forEach((part: any) => {
            if (part.node_name) {
              const nodes = Array.isArray(part.node_name) ? part.node_name : [part.node_name]
              if (nodes.includes(nodeName)) {
                bomName = part.bom_name
              }
            }
          })
        }
        
        // äº§å“æ€»è£…ï¼šä»componentså’Œfastenersä¸­æŸ¥æ‰¾
        if (step.components) {
          step.components.forEach((comp: any) => {
            if (comp.node_name) {
              const nodes = Array.isArray(comp.node_name) ? comp.node_name : [comp.node_name]
              if (nodes.includes(nodeName)) {
                bomName = comp.bom_name
              }
            }
          })
        }
        
        if (step.fasteners) {
          step.fasteners.forEach((fastener: any) => {
            if (fastener.node_name) {
              const nodes = Array.isArray(fastener.node_name) ? fastener.node_name : [fastener.node_name]
              if (nodes.includes(nodeName)) {
                bomName = fastener.bom_name
              }
            }
          })
        }
      })
      
      parts.push({
        nodeName,
        bomName
      })
    }
  })
  
  return parts
})

// æœç´¢è¿‡æ»¤
const filteredPartsList = computed(() => {
  if (!searchKeyword.value) return allPartsList.value
  
  const keyword = searchKeyword.value.toLowerCase()
  return allPartsList.value.filter(part => 
    part.bomName.toLowerCase().includes(keyword) ||
    part.nodeName.toLowerCase().includes(keyword)
  )
})

// è·å–é›¶ä»¶çš„å½“å‰é¢œè‰²æ ‡ç­¾
const getColorLabel = (nodeName: string) => {
  const customColors = currentStepData.value?.custom_colors || {}
  const currentHighlight = currentStepData.value?.['3d_highlight'] || []
  const assembled = assembledMeshes.value
  
  // ä¼˜å…ˆçº§1ï¼šç”¨æˆ·è‡ªå®šä¹‰é¢œè‰²
  if (customColors[nodeName]) {
    const color = customColors[nodeName]
    if (color === 'yellow') return 'ğŸŸ¡ é»„è‰²ï¼ˆè‡ªå®šä¹‰ï¼‰'
    if (color === 'green') return 'ğŸŸ¢ ç»¿è‰²ï¼ˆè‡ªå®šä¹‰ï¼‰'
    if (color === 'gray') return 'âšª ç°è‰²ï¼ˆè‡ªå®šä¹‰ï¼‰'
  }
  
  // ä¼˜å…ˆçº§2ï¼š3d_highlight
  if (currentHighlight.includes(nodeName)) {
    return 'ğŸŸ¡ é»„è‰²ï¼ˆæ­£åœ¨è£…é…ï¼‰'
  }
  
  // ä¼˜å…ˆçº§3ï¼šassembledMeshes
  if (assembled.includes(nodeName)) {
    return 'ğŸŸ¢ ç»¿è‰²ï¼ˆå·²è£…é…ï¼‰'
  }
  
  // ä¼˜å…ˆçº§4ï¼šé»˜è®¤ç°è‰²
  return 'âšª ç°è‰²ï¼ˆæœªè£…é…ï¼‰'
}

const getColorType = (nodeName: string) => {
  const label = getColorLabel(nodeName)
  if (label.includes('é»„è‰²')) return 'warning'
  if (label.includes('ç»¿è‰²')) return 'success'
  return 'info'
}

// å®æ—¶é¢„è§ˆ
const previewColors = () => {
  // ä¸´æ—¶ä¿å­˜åˆ°currentStepDataï¼Œè§¦å‘æ¸²æŸ“æ›´æ–°
  if (currentStepData.value) {
    currentStepData.value.custom_colors = { ...customColors.value }
  }
  ElMessage.success('é¢„è§ˆå·²æ›´æ–°ï¼Œè¯·æŸ¥çœ‹3Dè§†å›¾')
}

// æ¸…é™¤æ‰€æœ‰è‡ªå®šä¹‰é¢œè‰²
const clearCustomColors = () => {
  customColors.value = {}
  if (currentStepData.value) {
    currentStepData.value.custom_colors = {}
  }
  ElMessage.success('å·²æ¸…é™¤æ‰€æœ‰è‡ªå®šä¹‰é¢œè‰²')
}

// æ¢å¤é»˜è®¤
const resetToDefault = () => {
  customColors.value = { ...(currentStepData.value?.custom_colors || {}) }
  ElMessage.success('å·²æ¢å¤åˆ°ä¿å­˜çš„çŠ¶æ€')
}
```

